#finding the first 10 prime number
def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num ** 0.5) + 1): #check divisibility from 2 up to 
        if num % i == 0: ## If num is divisible by i, it's not prime
            return False
    return True

def find_primes(n):
    primes = []
    num = 2
    while len(primes) < n:
        if is_prime(num):
            primes.append(num)
        num += 1
    return primes

# Example usage
print(find_primes(10))
